{
  "realm": "kieserver",
  "realm-public-key": "",
  "bearer-only": true,
  "auth-server-url": "@@RHSSO_URL@@",
  "ssl-required": "all",
  "resource": "kieserver",
  "disable-trust-manager": false,
  "truststore" : "@@RHSSO_TRUSTSTORE@@",
  "truststore-password" : "@@RHSSO_TRUSTSTORE_PASSWORD@@",  
  "enable-cors" : true,
  "cors-max-age" : 1000,
  "cors-allowed-methods" : "POST, PUT, DELETE, GET",
  "principal-attribute" : "preferred_username"
}